{
  "expo": {
    "name": "AstroTune",
    "slug": "astrotune",
    "version": "0.4.0",
    "orientation": "default",
    "icon": "./src/assets/logo/half/icon_half.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./src/assets/logo//full/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#010526"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/logo/half/icon_half.png",
        "backgroundColor": "#010526"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.pchapman.astrotune",
      "permissions": [
        "FOREGROUND_SERVICE",
        "INTERNET",
        "NOTIFICATION",
        "WRITE_EXTERNAL_STORAGE",
        "READ_EXTERNAL_STORAGE"
      ],
      "services": {
        "musicservice": {
          "name": "MusicService",
          "exported": true
        }
      }
    },
    "web": {
      "favicon": "./src/assets/logo/full/icon.png"
    },
    "extra": {
      "eas": {
        "projectId": "79fec63c-c71e-41ab-8eb3-4e959253baae"
      }
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "android": {
            "usesCleartextTraffic": true
          }
        }
      ],
      "expo-secure-store"
    ]
  }
}