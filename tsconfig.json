{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "sourceMap": true,
    "baseUrl": "./src",
    "paths": {
      "screens/*": ["screens/*"],
      "classes/*": ["classes/*"],
      "managers/*": ["managers/*"],
      "elements/*": ["elements/*"],
      "enum/*": ["enum/*"],
      "helpers/*": ["helpers/*"],
      "hooks/*": ["hooks/*"],
      "style/*": ["style/*"],
      "assets/*": ["assets/*"]
